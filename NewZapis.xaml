<Window x:Class="MurzaevIR.NewZapis"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MurzaevIR"
        mc:Ignorable="d"
        Title="NewZapis" Height="250" Width="250">
    <Grid>
        <StackPanel HorizontalAlignment="Center">
            <TextBlock Name="TbTitle"></TextBlock>
            <TextBlock Name="TbDuration" Margin="0,10"></TextBlock>
            <ComboBox Name="CbUsers"></ComboBox>
            <DatePicker Name="DpDate" Margin="0,10"></DatePicker>
            <Button Margin="0,10" Click="Button_Click">Записать</Button>
        </StackPanel>
    </Grid>
</Window>
